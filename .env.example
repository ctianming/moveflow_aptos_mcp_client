# AI配置 - 替换为你的API密钥和基础URL
API_KEY=your_api_key_here
BASE_URL=https://api.deepseek.com/v1  # 或 https://api.openai.com/v1
MODEL=deepseek-chat  # 或 gpt-4, gpt-3.5-turbo 等

# Aptos配置
APTOS_PRIVATE_KEY=your_private_key_here  # 替换为你的私钥
APTOS_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1  # 测试网节点URL
APTOS_NETWORK=testnet  # 可选: mainnet, testnet, devnet
READ_ONLY_MODE=true  # true 表示只读模式，false 表示可执行交易
SIGNING_MODE=direct  # 签名模式: direct 或留空